		<script>
			document.addEventListener('DOMContentLoaded', function() {
				// Track App Store download button clicks
				const downloadButton = document.querySelector('.cta-button');
				if (downloadButton) {
					downloadButton.addEventListener('click', function() {
						// Track download button click
						if (window.location.hostname === 'gskis.com' && typeof mixpanel !== 'undefined') {
							mixpanel.track('Download Button Click', {
								page: 'myMeetings_landing',
								button_location: 'hero_section',
								button_text: 'Download on the App Store',
								url: window.location.href
							});
						}
						
						// Also track with console for debugging in development
						console.log('MyMeetings: Download button clicked');
					});
				}
				
				// Track SAMHSA button clicks
				const samhsaButton = document.querySelector('.contact-button');
				if (samhsaButton) {
					samhsaButton.addEventListener('click', function() {
						if (window.location.hostname === 'gskis.com' && typeof mixpanel !== 'undefined') {
							mixpanel.track('SAMHSA Link Click', {
								page: 'myMeetings_landing',
								button_location: 'contact_section',
								url: window.location.href
							});
						}
					});
				}
				
				// Track footer link clicks
				const footerLinks = document.querySelectorAll('.footer-links a');
				footerLinks.forEach(function(link) {
					link.addEventListener('click', function() {
						if (window.location.hostname === 'gskis.com' && typeof mixpanel !== 'undefined') {
							mixpanel.track('Footer Link Click', {
								page: 'myMeetings_landing',
								link_text: this.textContent.trim(),
								link_href: this.getAttribute('href'),
								url: window.location.href
							});
						}
					});
				});
			});
		</script>
